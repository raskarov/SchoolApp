@model SchoolApp.Models.UserProfile

@{
    ViewBag.Title = "Delete";
}

<div class="well">
    <h3>Вы уверены что хотите удалить учителя по имени @Html.DisplayFor(model => model.FullName)?</h3>
@using (Html.BeginForm()) {
    <p>
         <button type="submit" class="btn btn-primary"><i class="icon-trash"></i> Удалить</button>
         <a href="@Url.Action("Index")" class="btn">Назад к списку</a>
    </p>
}
    </div>