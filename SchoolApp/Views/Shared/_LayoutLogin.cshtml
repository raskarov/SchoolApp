<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - School App</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    @*Font used for logo*@
    <link href='http://fonts.googleapis.com/css?family=Dancing+Script:700' rel='stylesheet' type='text/css'>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    @Styles.Render("~/Content/bootstrap")
    @Scripts.Render("~/Bundles/bootstrap")
    @Styles.Render("~/Content/font-awesome")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Bundles/datatables")
    @Scripts.Render("~/bundles/multi-select")
    @Scripts.Render("~/bundles/noty")
    @Styles.Render("~/Content/datatables")
    @Styles.Render("~/Content/multi-select")
    @Styles.Render("~/Content/themes/base/css")
    @Styles.Render("~/Content/colorpicker")
    @Scripts.Render("~/bundles/colorpicker")
    @RenderSection("scripts", required: false)
    @RenderSection("styles", required: false)
    <link href="~/Content/login.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript">
        $(function () {
            $("[rel='tooltip']").tooltip();
        });
    </script>
</head>

<!--[if lt IE 7 ]> <body class="ie ie6"> <![endif]-->
<!--[if IE 7 ]> <body class="ie ie7 "> <![endif]-->
<!--[if IE 8 ]> <body class="ie ie8 "> <![endif]-->
<!--[if IE 9 ]> <body class="ie ie9 "> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<body class="">
    <!--<![endif]-->
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="row-fluid">
                <div class="login-box">
                    <div class="icons">
                        <i style="float:left; font-family: 'Dancing Script', cursive; font-size:24px">Speak English</i>
                        <a href="#"><i class="icon-home"></i></a>
                        <a href="#"><i class="icon-cog"></i></a>
                    </div>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
</body>
</html>
