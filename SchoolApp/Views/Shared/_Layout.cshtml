<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - School App</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/theme.css")
    @Styles.Render("~/Content/elements.css")
    @Styles.Render("~/Content/multi-select.css")
    @Scripts.Render("~/scripts/jquery.dataTables.js")
    @Scripts.Render("~/scripts/bootstrap.js")
    @Styles.Render("~/content/datatables.bootstrap.css")
    @Scripts.Render("~/scripts/site.js")
    @Scripts.Render("~/scripts/datatables.bootstrap.js")
    <!--[if IE 7 ]> 
            @Styles.Render("~/Content/font-awesome-ie7.css.css")
        <![endif]-->

    @Styles.Render("~/Content/font-awesome.css")


    @Scripts.Render("~/bundles/modernizr")
    <script type="text/javascript">
        $(function () {
            $("[rel='tooltip']").tooltip();
        });
    </script>
</head>

