        <a href="faq.html" class="nav-header"><i class="icon-comment"></i>Faq</a>
    </div>
    <div class="content">
        <div class="header">
            <div class="stats">

                @*Optional stats to display on page.*@
                @{if (ViewBag.Stats!=null)
                    {
                    foreach(var stat in ViewBag.Stats)
                    {
                        <p class="stat"><span class="number">@stat.Item1</span>@stat.Item2</p>
                    }
                    }
                }
            </div>
            @* Title of a page is defined in a view*@
            <h1 class="page-title">@ViewBag.Title</h1>
        </div>

        <ul class="breadcrumb">
            <li><a href="index.html">Test</a> <span class="divider">/</span></li>
            <li class="active">Location</li>
        </ul>
        <div class="container-fluid">
            <div class="row-fluid">
                @RenderSection("featured", false)
                @RenderBody()

                <footer>
                    <hr style="margin: 0;" />
                    <p class="pull-right">A <a href="http://www.portnine.com/bootstrap-themes" target="_blank">Free Bootstrap Theme</a> by <a href="http://www.portnine.com" target="_blank">Portnine</a></p>


                    <p>&copy; 2012 <a href="http://www.portnine.com" target="_blank">Portnine</a></p>
                </footer>

            </div>
        </div>
    </div>
</body>
</html>
